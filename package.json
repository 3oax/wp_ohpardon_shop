{
  "name": "oax-template",
  "version": "0.0.1",
  "description": "",
  "main": "",
  "browserslist": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ],
  "eslintConfig": {
    "root": true,
    "extends": [
      "inpsyde"
    ],
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "globals": {
      "process": false
    },
    "env": {
      "browser": true,
      "mocha": true
    },
    "rules": {
      "linebreak-style": 0,
      "lines-around-comment": 0,
      "capitalized-comments": 0,
      "class-methods-use-this": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "no-var": 0,
      "one-var": 0,
      "line-comment-position": 0,
      "no-inline-comments": 0,
      "no-mixed-operators": 0,
      "no-redeclare": 0,
      "block-scoped-var": 0,
      "vars-on-top": 0,
      "no-trailing-spaces": 0,
      "space-before-blocks": 0,
      "max-statements": 0,
      "computed-property-spacing": 0,
      "space-before-function-paren": 0,
      "yoda": 0,
      "valid-jsdoc": 0,
      "space-in-parens": [
        1,
        "always"
      ]
    }
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@barba/prefetch": "^2.1.10",
    "glightbox": "^3.2.0",
    "gsap": "file:assets/js/lib/gsap/npm-install-this/gsap-bonus.tgz",
    "headroom.js": "^0.9.4",
    "imagesloaded": "^4.1.4",
    "is-in-viewport": "^3.0.4",
    "lazysizes": "^5.1.0",
    "lightgallery": "^1.6.12",
    "modernizr": "^3.7.1",
    "plyr": "^3.6.8",
    "smooth-scrollbar": "^8.6.1",
    "svg-injector-2": "^2.1.3",
    "swiper": "^8.0.6",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "autoprefixer": "^9.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "backstopjs": "^3.9.13",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "buble-loader": "^0.4.1",
    "cache-loader": "^1.0.3",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-globs-webpack-plugin": "^0.2.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.8.0",
    "eslint": "^4.7.0",
    "eslint-config-inpsyde": "^2.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.18.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "ify-loader": "^1.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webpack-plugin": "^1.5.2",
    "import-glob": "^1.5.0",
    "inquirer": "^4.0.0",
    "intrinsic-scale": "^3.0.4",
    "json-loader": "^0.5.7",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.4",
    "mini-css-extract-plugin": "^1.6.2",
    "mocha": "^4.0.1",
    "modernizr-loader": "^1.0.1",
    "node-sass": "npm:sass@^1.43.4",
    "node-wp-i18n": "^1.2.3",
    "postcss-css-variables": "^0.18.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-safe-parser": "^3.0.1",
    "puppeteer": "^0.13.0",
    "replace-in-file": "^3.4.4",
    "resolve-url-loader": "^2.3.2",
    "sass": "^1.43.4",
    "sass-loader": "^6.0.7",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.46.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "yargs": "^10.0.3"
  },
  "scripts": {
    "build": "webpack --progress --config assets/build/webpack.config.js",
    "build:production": "webpack --progress -p --config assets/build/webpack.config.js",
    "build:profile": "webpack --progress --profile --config assets/build/webpack.config.js",
    "start": "webpack --watch --config assets/build/webpack.config.js",
    "lint": "eslint assets/js",
    "test": "karma start assets/build/karma.conf.js",
    "makepot": "wpi18n makepot --domain-path assets/languages --pot-file en_US.pot",
    "visual:ref": "backstop --configPath=assets/build/backstop.json reference",
    "visual:test": "backstop --configPath=assets/build/backstop.json test"
  },
  "author": "threeOax",
  "license": "GPL"
}
